<?xml version="1.0" encoding="utf-8"?>
<com.xpg.hssy.view.PilesShowInfoView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="@dimen/w220"
    android:layout_height="wrap_content"
    android:layout_gravity="center"
    android:background="@drawable/bg_chart_pop"
    android:gravity="center"
    android:orientation="vertical"
    android:padding="@dimen/h20" >

    <ScrollView
        android:id="@+id/scrollView_showMessages"
        android:layout_width="@dimen/w220"
        android:layout_height="@dimen/h130"
        android:layout_gravity="center"
        android:gravity="center"
        android:paddingBottom="@dimen/h10"
        android:scrollbars="vertical" >

        <LinearLayout
            android:id="@+id/listlinear"
            android:layout_width="@dimen/w220"
            android:layout_height="wrap_content"
            android:divider="@drawable/line1"
            android:orientation="vertical"
            android:showDividers="middle|end" >
        </LinearLayout>
    </ScrollView>

</com.xpg.hssy.view.PilesShowInfoView>